<%= simple_form_for @car do |f| %>
  <%= f.input :title %>
  <%= f.input :photo %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :motor %>
  <%= f.input :fuel %>
  <%= f.input :brand %>
  <%= f.input :year %>
  <%= f.input :modele %>
  <%= f.input :power %>
  <%= f.button :submit %>
<% end %>
