<div class="container">
  <h1> Réservation : <%= @booking.start_date %> <%= @booking.end_date %></h1>
  <p><%= @car.title %></p>
  <p><%= @car.price %> € par jour</p>
  <p><%= @car.description %></p>
  <p>Marque: <%= @car.brand %></p>
  <p>Modèle: <%= @car.modele %></p>
  <p>Motorisation: <%= @car.motor %></p>
  <p>Carburant: <%=@car.fuel %></p>
  <p>Puissance DIN: <%= @car.power %>Ch</p>
  <%= link_to "Add a Review", new_booking_review_path(booking_id: @booking.id) %>
</div>
